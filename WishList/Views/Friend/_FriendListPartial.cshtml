@model IQueryable<WishList.ViewModels.UserViewModel>

@foreach (var md in Model)
{
    <div id=@md.Id>
        <h4>@md.FirstName</h4>
        <h4>@md.LastName</h4>
        <h4>@md.FormattedBirthday</h4>
        <h4>@md.Avatar</h4>
        @Ajax.ActionLink("delete", "DeleteFriend",  new { id = md.Id }, null)
    </div>
}